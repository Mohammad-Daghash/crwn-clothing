{
    "name": "crwn-clothing-server",
    "version": "1.0.0",
    "scripts": {
        "client": "cd client && npm start",
        "server": "nodemon server.js",
        "build": "cd client && npm i && npm run build && cd ../node server.js",
        "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
        "start": "node server.js"
    },
    "dependencies": {
        "body-parser": "^1.20.2",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "stripe": "^11.14.0"
    },
    "devDependencies": {
        "concurrently": "^7.6.0"
    },
    "engines": {
        "node": "^16.16.0",
        "npm": "^8.11.0"
    }
}
